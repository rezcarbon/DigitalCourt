{
  "id": "cogflow-001",
  "name": "Cognitive Flow Orchestration",
  "version": "1.0.0",
  "description": "Core orchestration layer for cognitive processing flows within the Digital Court",
  "components": [
    {
      "name": "Input Processing",
      "type": "processor",
      "priority": "high",
      "config": {
        "max_concurrent": 10,
        "timeout": 30000
      }
    },
    {
      "name": "Cognitive Analysis",
      "type": "analyzer",
      "priority": "critical",
      "config": {
        "depth": "deep",
        "parallel": true
      }
    },
    {
      "name": "Output Generation",
      "type": "generator",
      "priority": "medium",
      "config": {
        "formatting": "adaptive",
        "validation": true
      }
    }
  ],
  "routing_policies": {
    "default": "round_robin",
    "overflow": "queue",
    "error_handling": "retry_then_fail"
  }
}